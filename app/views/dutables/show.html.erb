
    <h1><%=h @dutable.du_name %></h1>
    <table class="table table-bordered table-hover">
      <thead>
        <tr>
          <th>Sr No.</th>
          <th>Projects</th>
          <th>Total Requirements</th>
          <th>Action</th>



        </tr>
      </thead>
      <tbody>
      <% @dutable.projects.each_with_index do |project, i|%>
          <tr>
            <td>
              <%= i + 1%>
            </td>
            <td>
              <%= project.project_name%>
            </td>
            <td>
              <span class="badge badge-warning badge-left" ><%=project.requirements.count%></span>
            </td>
            <td>
            <%= link_to "Details".html_safe, project_path(project), class:'btn btn-info fa fa-eye'%>
            </td>



        <% end %>
      </tbody>
    </table>
