<h1>Listing candidates</h1>
<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Candidate Full_Name</th>

      <th>Contact</th>
      <th>Email</th>




    </tr>
  </thead>
  <tbody>
    <%  @candidate.each do |candidate| %>
      <tr>
        <td>
          <%= candidate.candidate_full_name %>
        </td>

        <td>
          <%= candidate.contact %>

        </td>
        <td>
          <%= candidate.email %>


        </td>

        <td>
          <%=candidate.feedback.status %>
        </td>



      </tr>
    <% end %>
  </tbody>
</table>
<br/>
